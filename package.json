{
  "name": "nrequire",
  "version": "1.1.2",
  "description": "Automatic `require`. Installs dependencies from git and NPM without NPM",
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha --require blanket -R mocha-spec-cov-alt"
  },
  "website": "https://github.com/yamadapc/nrequire",
  "repository": "git@github.com:yamadapc/nrequire.git",
  "author": "Pedro Tacla Yamada",
  "license": "MIT",
  "devDependencies": {
    "blanket": "^1.1.6",
    "mocha": "^2.1.0",
    "mocha-make-stub": "^2.3.2",
    "mocha-spec-cov-alt": "^0.3.0",
    "remove": "^0.1.5",
    "should": "^6.0.1"
  },
  "config": {
    "blanket": {
      "data-cover-never": [
        "node_modules"
      ],
      "pattern": [
        "lib",
        "YOUR",
        "MODULE",
        "DIRECTORIES",
        "HERE"
      ],
      "spec-cov": {
        "threshold": 80,
        "localThreshold": 80
      }
    }
  }
}
